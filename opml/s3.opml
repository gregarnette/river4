					<outline created="Sat, 22 Mar 2014 16:52:18 GMT" pgfnum="39743" text="console.log (&quot;s3NewObject: error == &quot; + err.message);"></outline>
					<outline text="s3stats.ctWriteErrors++;"></outline>
