										<outline text="&lt;/div&gt;"></outline>
										</outline>
									<outline text="{{each(f,feed) updatedFeeds.updatedFeed}}"></outline>
									<outline text="&lt;div class=&quot;section&quot;&gt;">
										<outline text="&lt;div class=&quot;header&quot;&gt;">
											<outline text="&lt;h2&gt;">
												<outline text="{{if feed.websiteUrl}}&lt;a href=&quot;${feed.websiteUrl}&quot;&gt;{{/if}}&lt;img class=&quot;icon&quot; src=&quot;${getFavicon(feed.websiteUrl)}&quot; alt=&quot;&quot;&gt;{{if feed.websiteUrl}}&lt;/a&gt;{{/if}}"></outline>
												<outline text="{{if feed.websiteUrl}}&lt;a href=&quot;${feed.websiteUrl}&quot;&gt;{{/if}}${feed.feedTitle}{{if feed.websiteUrl}}&lt;/a&gt;{{/if}}"></outline>
												<outline text="(&lt;a href=&quot;${feed.feedUrl}&quot; title=&quot;Subscribe to ${feed.feedTitle}&quot;&gt;Feed&lt;/a&gt;)"></outline>
												</outline>
											<outline text="&lt;/h2&gt;"></outline>
											<outline text="&lt;span class=&quot;time&quot;&gt;${dateFormat(feed.whenLastUpdate, 'timeDate')}&lt;/span&gt;"></outline>
											<outline text="&lt;/div&gt;"></outline>
											</outline>
										<outline text="{{each(i,item) feed.item}}"></outline>
										<outline text="&lt;div class=&quot;article&quot;&gt;">
											<outline text="&lt;div class=&quot;header&quot;&gt;">
												<outline text="&lt;h3&gt;&lt;a href=&quot;{{if item.permaLink}}${item.permaLink}{{else}}${item.link}{{/if}}&quot; rel=&quot;external&quot;&gt;{{if item.title}}${item.title}{{else}}${item.body}{{/if}}&lt;/a&gt;&lt;/h3&gt;"></outline>
												<outline text="&lt;/div&gt;"></outline>
												</outline>
											<outline text="{{if item.title}}"></outline>
											<outline text="{{if item.body}}"></outline>
											<outline text="&lt;div class=&quot;description&quot;&gt;">
												<outline text="{{each(t,thumbnail) item.thumbnail}}"></outline>
												<outline text="&lt;img class=&quot;thumbnail&quot; src=&quot;${thumbnail.url}&quot; width=&quot;${thumbnail.width}&quot; height=&quot;${thumbnail.height}&quot; alt=&quot;&quot;&gt;"></outline>
												<outline text="{{/each}}"></outline>
												<outline text="${getText(item.body)}"></outline>
												</outline>
											<outline text="&lt;/div&gt;"></outline>
											<outline text="{{/if}}"></outline>
											<outline text="{{/if}}"></outline>
											<outline text="&lt;div class=&quot;footer&quot;&gt;">
												<outline text="&lt;span class=&quot;time&quot;&gt;${timeDifference(item.pubDate)}&lt;/span&gt;"></outline>
												<outline text="&lt;ul class=&quot;actions&quot;&gt;">
													<outline text="{{if item.comments}}"></outline>
													<outline text="&lt;li&gt;&lt;a href=&quot;${item.comments}&quot; rel=&quot;external nofollow&quot; target=&quot;_blank&quot;&gt;Comment&lt;/a&gt;&lt;/li&gt;"></outline>
													<outline text="{{/if}}"></outline>
													<outline text="{{each(e,enclosure) item.enclosure}}"></outline>
													<outline text="&lt;li class=&quot;enclosure ${getEnclosureType(enclosure.type)}&quot;&gt;&lt;a href=&quot;${enclosure.url}&quot;&gt;Download enclosure (${enclosure.type}, ${getEnclosureSize(enclosure.length)})&lt;/a&gt;&lt;/li&gt;"></outline>
													<outline text="{{/each}}"></outline>
													</outline>
												<outline text="&lt;/ul&gt;"></outline>
												</outline>
											<outline text="&lt;/div&gt;"></outline>
											</outline>
