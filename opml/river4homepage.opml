									<outline text="&lt;div class=&quot;loader&quot;&gt;&lt;/div&gt;"></outline>
									<outline text="&lt;!-- news feed via template inserted here --&gt; "></outline>
									<outline text="&lt;/div&gt;"></outline>
									</outline>
								<outline text="&lt;footer id=&quot;footer&quot;&gt;">
									<outline text="&lt;p&gt;The design and implementation of the first beautiful River2 template was a community project. &lt;a href=&quot;http://beautriver1.blorkmark.com/&quot;&gt;Credits&lt;/a&gt;.&lt;/p&gt;"></outline>
									<outline text="&lt;/footer&gt;"></outline>
									</outline>
								<outline text="&lt;script&gt;">
									<outline text="var jsonFeed = 'http://static.scripting.com/river3/rivers/fargo.js?callback=';"></outline>
